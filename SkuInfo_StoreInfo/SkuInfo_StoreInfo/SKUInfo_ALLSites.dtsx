              <properties>
                <property
                  dataType="System.Int32"
                  description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                  name="CommandTimeout">0</property>
                <property
                  dataType="System.String"
                  description="Specifies the name of the database object used to open a rowset."
                  name="OpenRowset"></property>
                <property
                  dataType="System.String"
                  description="Specifies the variable that contains the name of the database object used to open a rowset."
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT distinct
result.RemoteSystemId,
result.SourceId,
result.Upc,
result.VariantUpcId,
result.VariantID,
isnull(result.ProductId,0) ProductId,
isnull(result.producttypeid,0)[ProductTypeId],
isnull(ptn.Name,'Not Set') ProductTypeName
FROM centralpimuat.dbo.variant v1
inner join
(
                select distinct
                                case when vsc.Variantid is null then '000' else  vsc.Variantid end as VariantID,
                                Case when vsc.RemotesystemId is null then '000' else vsc.RemotesystemId end RemoteSystemId,
                                Case when vsc.SourceId is null then '000' else vsc.SourceId end SourceId,
                                Case when vupc.VariantUpcId is null then '000' else vupc.VariantUpcId end  VariantUpcId,
                                Case when vupc.Upc is null then '000' else vupc.upc end Upc
                                ,max(pp.ProductTypeId) ProductTypeId
                                , v.ProductId
                FROM
                centralpimuat.dbo.variantsite vs with (nolock)
                inner join  centralpimuat.dbo.[Site] s with (nolock) on s.siteid =  vs.siteid
                inner join centralpimuat.dbo.variant v with (nolock) on vs.variantid=v.variantid
                inner join centralpimuat.dbo.variantsource vsc with (nolock) on vsc.variantid=v.variantid
               outer apply (select top 1 ppt.productTypeId  from  centralpimuat.dbo.ProductProductTypes  (nolock) ppt
			   inner join centralpimuat.dbo.productsite  ps with (nolock) on ps.productid=v.productid and ps.SiteId in (1,4,7,10,11,14,16,50,60,68,70,72,73,74,105,108,133,142,143,146)
			   where  ppt.ProductSiteId =ps.ProductSiteId ) as PP
                inner join centralpimuat.dbo.variantupc vupc with (nolock) on vupc.variantid=v.variantid
                where vs.siteid in (1,4,7,10,11,14,16,50,60,68,70,72,73,74,105,108,133,142,143,146)and vsc.sourceid=1 and vupc.UpcTypeId=1
                group by v.productid ,vsc.Variantid ,vsc.Variantid ,vsc.RemotesystemId ,vsc.RemotesystemId ,vsc.SourceId ,vupc.VariantUpcId ,vupc.Upc--,pp.name
) AS result  on v1.VariantId = result.VariantID
left join centralpimuat.[dbo].[ProductTypeName] ptn on ptn.ProductTypeId = result.ProductTypeId and ptn.LanguageId = 1
order by RemoteSystemId desc</property>
